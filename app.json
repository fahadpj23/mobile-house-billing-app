{
  "expo": {
    "name": "mobileHouseBillingApp",
    "slug": "mobileHouseBillingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MobilehouseLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.fahadpj.mobileHouseBillingApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d7d4012e-151b-4800-8d0f-c0c4ce6bdaf3"
      }
    }
  }
}
